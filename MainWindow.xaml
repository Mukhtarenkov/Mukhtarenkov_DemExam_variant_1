<Window x:Class="Mukhtarenkov_DemExam_variant_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mukhtarenkov_DemExam_variant_1"
        mc:Ignorable="d"
        Title="Автосервис" Height="450" Width="800" MaxHeight="700" MinHeight="450" MaxWidth="1200" MinWidth="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Frame x:Name="MainFrame" Grid.Row="1" />
        <Button x:Name="GoBack" Content="назад" Click="GoBack_Click" Grid.Row="2"/>
    </Grid>
</Window>
